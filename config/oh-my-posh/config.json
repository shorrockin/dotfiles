{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "os": "#ACB0BE",
    "closer": "p:os",
    "pink": "#F5C2E7",
    "lavender": "#B4BEFE",
    "blue": "#89B4FA",
    "green": "#A6E3A1",
    "yellow": "#F9E2AF",
    "red": "#F38BA8",
    "sky": "#89DCEB",
    "mauve": "#CBA6F7",
    "peach": "#FAB387",
    "text": "#CDD6F4"
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "p:os",
          "style": "plain",
          "template": "{{.Icon}} ",
          "type": "os"
        },
        {
          "foreground": "p:pink",
          "options": {
            "folder_icon": "..\ue5fe..",
            "home_icon": "~",
            "style": "agnoster_short"
          },
          "style": "plain",
          "template": "{{ .Path }} ",
          "type": "path"
        },

        {
          "type": "git",
          "style": "plain",
          "foreground": "p:blue",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:sky{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:red{{ end }}",
            "{{ if gt .Ahead 0 }}p:sky{{ end }}",
            "{{ if gt .Behind 0 }}p:pink{{ end }}"
          ],
          "background": "transparent",
          "template": "{{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }}  {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}  {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }}  {{ .StashCount }}{{ end }} ",
          "properties": {
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "branch_icon": " ",
            "branch_identical_icon": "",
            "branch_ahead_icon": "⇡",
            "branch_behind_icon": "⇣",
            "branch_gone_icon": "✘",
            "commit_icon": " ",
            "tag_icon": " ",
            "rebase_icon": " ",
            "cherry_pick_icon": " ",
            "revert_icon": " ",
            "merge_icon": " ",
            "no_commits_icon": " "
          }
        },
        {
          "type": "status",
          "style": "plain",
          "foreground": "p:red",
          "foreground_templates": [
            "{{ if eq .Env.OMP_FISH_BIND_MODE \"default\" }}p:os{{ end }}",
            "{{ if eq .Env.OMP_FISH_BIND_MODE \"visual\" }}p:mauve{{ end }}",
            "{{ if eq .Env.OMP_FISH_BIND_MODE \"replace_one\" }}p:red{{ end }}",
            "{{ if eq .Env.OMP_FISH_BIND_MODE \"replace\" }}p:red{{ end }}",
            "{{ if gt .Code 0 }}p:red{{ end }}"
          ],
          "background": "transparent",
          "template": "❯",
          "properties": {
            "always_enabled": true
          }
        }
      ],
      "type": "prompt"
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "node",
          "style": "plain",
          "foreground": "p:green",
          "background": "transparent",
          "template": " {{ .Full }} "
        },
        {
          "type": "python",
          "style": "plain",
          "foreground": "p:yellow",
          "background": "transparent",
          "template": " {{ .Full }} "
        },
        {
          "type": "go",
          "style": "plain",
          "foreground": "p:blue",
          "background": "transparent",
          "template": " {{ .Full }} "
        },
        {
          "type": "rust",
          "style": "plain",
          "foreground": "p:red",
          "background": "transparent",
          "template": " {{ .Full }} "
        },
        {
          "type": "java",
          "style": "plain",
          "foreground": "p:red",
          "background": "transparent",
          "template": " {{ .Full }} "
        },
        {
          "type": "ruby",
          "style": "plain",
          "foreground": "p:red",
          "background": "transparent",
          "template": " {{ .Full }} "
        },
        {
          "type": "elixir",
          "style": "plain",
          "foreground": "p:mauve",
          "background": "transparent",
          "template": " {{ .Full }} "
        }
      ]
    }
  ],
  "transient_prompt": {
    "background": "transparent",
    "foreground": "p:red",
    "foreground_templates": [
      "{{ if eq .Env.OMP_FISH_BIND_MODE \"default\" }}p:os{{ end }}",
      "{{ if eq .Env.OMP_FISH_BIND_MODE \"visual\" }}p:mauve{{ end }}",
      "{{ if eq .Env.OMP_FISH_BIND_MODE \"replace_one\" }}p:red{{ end }}",
      "{{ if eq .Env.OMP_FISH_BIND_MODE \"replace\" }}p:red{{ end }}",
      "{{ if gt .Code 0 }}p:red{{ end }}"
    ],
    "template": "❯ "
  },
  "final_space": true,
  "version": 4
}
